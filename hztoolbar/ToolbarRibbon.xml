<?xml version="1.0" encoding="UTF-8"?>
<customUI xmlns="http://schemas.microsoft.com/office/2009/07/customui" onLoad="Ribbon_Load">
	<ribbon>
		<tabs>
			<tab id="HZToolbar" label="H&amp;Z Toolbar">


				<group id="text_actions" getLabel="GetLabel">
					<button id="swap_text" getLabel="GetLabel" onAction="OnAction" getEnabled="IsEnabled" getImage="GetImage"  getSupertip="GetSupertip" size="large"/>
					<button id="clear_text" getLabel="GetLabel" onAction="OnAction" getEnabled="IsEnabled" getImage="GetImage"  getSupertip="GetSupertip" size="large"/>

					<button id="change_language_en" tag="change_language:en" getLabel="GetLabel" onAction="OnAction" getEnabled="IsEnabled" getImage="GetImage"  getSupertip="GetSupertip" size="large"/>
					<button id="change_language_de" tag="change_language:de" getLabel="GetLabel" onAction="OnAction" getEnabled="IsEnabled" getImage="GetImage"  getSupertip="GetSupertip" size="large"/>
				</group>

				<group id="shape_actions" getLabel="GetLabel" >
					<splitButton id="change_shape_actions">
						<button id="change_shape_repeat" imageMso="ShapesMoreShapes"/>

						<menu id="change_shape_type" getLabel="GetLabel" imageMso="ShapesMoreShapes" >
							<button id="change_to_rectangle" tag="change_shape_type:Rectangle" imageMso="ShapeRectangle" getLabel="GetLabel" onAction="OnAction" getEnabled="IsEnabled" getSupertip="GetSupertip" showLabel="false"  />
							<button id="change_to_roundedrectangle" tag="change_shape_type:RoundedRectangle" imageMso="ShapeRoundedRectangle" getLabel="GetLabel" onAction="OnAction" getEnabled="IsEnabled" getSupertip="GetSupertip" showLabel="false" />

							<button id="change_to_isoscelestriangle" tag="change_shape_type:IsoscelesTriangle" imageMso="ShapeIsoscelesTriangle" getLabel="GetLabel" onAction="OnAction" getEnabled="IsEnabled" getSupertip="GetSupertip" showLabel="false" />
							<button id="change_to_oval" tag="change_shape_type:Oval" imageMso="ShapeOval" getLabel="GetLabel" onAction="OnAction" getEnabled="IsEnabled" getSupertip="GetSupertip" showLabel="false" />
							<button id="change_to_smilyface" tag="change_shape_type:SmileyFace" imageMso="ShapeSmileyFace" getLabel="GetLabel" onAction="OnAction" getEnabled="IsEnabled" getSupertip="GetSupertip" showLabel="false" />
							<button id="change_to_donut" tag="change_shape_type:Donut" imageMso="ShapeDonut" getLabel="GetLabel" onAction="OnAction" getEnabled="IsEnabled" getSupertip="GetSupertip" showLabel="false" />
							<button id="change_to_heart" tag="change_shape_type:Heart" imageMso="ShapeHeart" getLabel="GetLabel" onAction="OnAction" getEnabled="IsEnabled" getSupertip="GetSupertip" showLabel="false" />
							<button id="change_to_rightarrow" tag="change_shape_type:RightArrow" imageMso="ShapeRightArrow" getLabel="GetLabel" onAction="OnAction" getEnabled="IsEnabled" getSupertip="GetSupertip" showLabel="false" />
							<button id="change_to_leftarrow" tag="change_shape_type:LeftArrow" imageMso="ShapeLeftArrow" getLabel="GetLabel" onAction="OnAction" getEnabled="IsEnabled" getSupertip="GetSupertip" showLabel="false" />
							<button id="change_to_uparrow" tag="change_shape_type:UpArrow" imageMso="ShapeUpArrow" getLabel="GetLabel" onAction="OnAction" getEnabled="IsEnabled" getSupertip="GetSupertip" showLabel="false" />
							<button id="change_to_downarrow" tag="change_shape_type:DownArrow" imageMso="ShapeDownArrow" getLabel="GetLabel" onAction="OnAction" getEnabled="IsEnabled" getSupertip="GetSupertip" showLabel="false" />

							<button id="change_to_roundedrectangularcallout" tag="change_shape_type:RoundedRectangularCallout" imageMso="ShapeRoundedRectangularCallout" getLabel="GetLabel" onAction="OnAction" getEnabled="IsEnabled" getSupertip="GetSupertip" showLabel="false" />
							<button id="change_to_start" tag="change_shape_type:5pointStar" imageMso="ShapeStar" getLabel="GetLabel" onAction="OnAction" getEnabled="IsEnabled" getSupertip="GetSupertip" showLabel="false" />
							<button id="change_to_seal8" tag="change_shape_type:8pointStar" imageMso="ShapeSeal8" getLabel="GetLabel" onAction="OnAction" getEnabled="IsEnabled" getSupertip="GetSupertip" showLabel="false" />
							<button id="change_to_seal16" tag="change_shape_type:16pointStar" imageMso="ShapeSeal16" getLabel="GetLabel" onAction="OnAction" getEnabled="IsEnabled" getSupertip="GetSupertip" showLabel="false" />
							<button id="change_to_seal24" tag="change_shape_type:24pointStar" imageMso="ShapeSeal24" getLabel="GetLabel" onAction="OnAction" getEnabled="IsEnabled" getSupertip="GetSupertip" showLabel="false" />
						</menu>
					</splitButton>






					<separator id="shape_actions_size_separator"/>


					<button id="harmonize_shape_width" getLabel="GetLabel" onAction="OnAction" getEnabled="IsEnabled" getImage="GetImage" getSupertip="GetSupertip" size="large"/>
					<button id="harmonize_shape_height" getLabel="GetLabel" onAction="OnAction" getEnabled="IsEnabled" getImage="GetImage" getSupertip="GetSupertip" size="large"/>
					<button id="harmonize_shape_size" getLabel="GetLabel" onAction="OnAction" getEnabled="IsEnabled" getImage="GetImage" getSupertip="GetSupertip" size="large"/>

					<button id="harmonize_adjustments" getLabel="GetLabel" onAction="OnAction" getEnabled="IsEnabled" getImage="GetImage" getSupertip="GetSupertip" size="large"/>

					<separator id="shape_actions_split_separator"/>

					<button id="split_rectangle" getLabel="GetLabel" onAction="OnAction" getEnabled="IsEnabled" getImage="GetImage" getSupertip="GetSupertip" size="large"/>
				</group>

				<group id="arrange_actions" getLabel="GetLabel">
					<button id="arrange_horizontal" getLabel="GetLabel" onAction="OnAction" getEnabled="IsEnabled" getImage="GetImage" getSupertip="GetSupertip" size="large"/>
